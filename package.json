{
  "name": "vuejs-ava-avoriaz-moxios",
  "version": "1.0.0",
  "description": "unit test for vuejs",
  "scripts": {
    "spec": "npm-run-all -p spec:*",
    "spec:delcoverage": "rimraf test/coverage",
    "spec:unit": "nyc ava"
  },
  "ava": {
    "babel": "inherit",
    "verbose": true,
    "require": [
      "./test/helpers/setup.js"
    ]
  },
  "nyc": {
    "report-dir": "test/coverage",
    "reporter": [
      "html",
      "lcov",
      "text"
    ],
    "extension": [
      ".js",
      ".vue"
    ]
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "avoriaz": "^2.4.2",
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "browser-env": "^3.1.0",
    "moxios": "^0.4.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.0.3",
    "require-extension-hooks": "^0.2.0",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^0.3.1",
    "rimraf": "^2.6.1",
    "vue-loader": "^13.0.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.4"
  },
  "dependencies": {
    "vue": "^2.3.4"
  }
}
